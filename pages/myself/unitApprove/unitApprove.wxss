/* pages/myself/unitCertification /unitCertification .wxss */
page{
  background-color: #ffffff;
}
.head{
  width: 220rpx;
  height: 215rpx;
  margin: 63rpx auto 56rpx
}
.desc{
  font-size: 24rpx;
  color: #666666;
  text-align: center;
  line-height: 48rpx;
}
.search{
  display: flex;
  align-items: center;
  padding: 0 40rpx;
  margin-top: 64rpx;
  position: relative;
}
.searchipt{
  flex: 1;
  font-size: 24rpx;
  padding: 11rpx;
  border: 1px solid #dcdcdc;
  position: relative;
}
.scrollview{
  position: absolute;
  top: 70rpx;
  line-height: 48rpx;
}
.down-icon{
  color: #ccc;
  position: absolute;
  right: 0;
  top: 0;
  border-left: 1px solid #ccc;
  padding: 0 18rpx;
  line-height: 60rpx;
}
.btnbox {
  padding: 0 75rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

button.submit {
  width: 100%;
  line-height: inherit;
  border-radius: 80rpx;
  color: #fff;
  font-size: 36rpx;
  font-weight: 400;
  background-color: #01bf6c;
  margin-bottom: 40rpx;
}