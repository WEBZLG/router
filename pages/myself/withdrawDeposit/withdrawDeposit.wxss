  padding: 30rpx 0;
}
.trip{
  font-size: 24rpx;
  color: #999999;
  padding: 0 40rpx;
}

.btnbox{
  margin: 226rpx 75rpx 0;
  text-align: center;
  font-size: 36rpx;
  color: #01BF6C;
}

.btnbox .greenbtn{
  margin-bottom: 60rpx;
}
.placeholder{
  font-size: 24rpx;
  color: #ffffff;
}
.money input{
  height: 100rpx;
}