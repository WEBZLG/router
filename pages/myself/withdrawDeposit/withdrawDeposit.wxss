page {
  background-color: #f5f5f5;
}

.container {
  padding: 30rpx 40rpx;
}

.account {
  padding: 0 30rpx;
  line-height: 90rpx;
  background-color: #efefef;
}

.account text {
  margin-right: 30rpx;
}

.moneybg {
  background-color: #fff;
  padding: 30rpx;
}
.total{
  padding-top: 30rpx;
  border-top: 1px solid #cccccc;
}
.money{
