/* pages/myself/nameApprove/nameApprove.wxss */
page{
  background-color: #ffffff;
}
.cardbg{
  height: 500rpx;
  width: 100%;
}
.iptbox{
  text-align: center;
}
.card{
  width: 430rpx;
  height: 280rpx;
  margin: 65rpx auto;
}
.btn{
    margin: 0 75rpx 62rpx;
}
.submit{
  background-color: #00C069;
  font-size: 36rpx;
  color: #ffffff;
  width: 100% !important;
  border-radius: 90rpx;
  font-weight: 400;
}
.title{
  font-size: 36rpx;
  color: #333333;
  margin: 60rpx 0 43rpx;
}

.iptbox input{
  border: 1px solid #EBEBEB;
  box-shadow: 0 -1px 0 1px #EBEBEB;
  font-size: 30rpx;
  color: #333333;
  padding: 20rpx 0;
  border-radius: 90rpx;
  margin: 0 75rpx;
}