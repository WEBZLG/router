/* pages/home/customize/customize.wxss */
.banner{
  width: 100%;
  height: 1548rpx;
}
.targetbox{
  background-color: #ffffff;
  padding: 80rpx 40rpx 32rpx;
}

.target{
  margin-bottom: 50rpx;
}

.target text,
.title{
  font-size: 48rpx;
  color: #333333;
}
.target view{
  font-size: 30rpx;
  color: #01BF6C;
}

.address{
  flex-wrap: wrap;
}
.item{
  height: 70rpx;
  width: 150rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 30rpx;
  border: 1px solid #E6E6E6;
  border-radius: 10rpx;
  background-color: #F6F6F6;
  margin-bottom: 30rpx;
}

.deepTraevl{
  padding: 60rpx 40rpx;
}

.title{
  margin-bottom: 45rpx;
}

.deepItem{
  width: 100%;
  height: 400rpx;
  margin-bottom: 30rpx;
  position: relative;
}

.mark{
  position: absolute;
  top: 137rpx;
  left: 0;
  right: 0;
  text-align: center;
  color: #ffffff;
}
.caption{
  font-size: 60rpx;
  margin-bottom: 36rpx;
}
.descript{
  font-size: 30rpx;
}