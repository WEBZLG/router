<!--pages/router/addPeople/addPeople.wxml-->
<view>
  <view class="title">基本信息</view>
  <view class="item">
    <text>姓名</text>
    <view>{{dataList.name}}</view>
  </view>
  <view class="item">
    <text>性别</text>
    <view>{{dataList.sex}}</view>
  </view>
  <view class="item">
    <text>身份证号码</text>
    <view>{{dataList.card_num}}</view>
  </view>
  <view class="item">
    <text>国家地区</text>
    <view>{{dataList.province}}{{dataList.city}}</view>
  </view>
  <view class="title">联系方式</view>
  <view class="item">
    <text>手机号码</text>
    <view>{{dataList.phone}}</view>
  </view>
  <view class="item">
    <text>固定电话</text>
    <view>{{dataList.tel}}</view>
  </view>
  <view class="item">
    <text>电子邮箱</text>
    <view>{{dataList.email}}</view>
  </view>
  <view class="item">
    <text>地址</text>
    <view>{{dataList.address}}</view>
  </view>
</view>